der=\"请输入活动库存\"/>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"选择关联商品\">\r\n      <a-select\r\n        v-model:value=\"form.pid\"\r\n        show-search\r\n        placeholder=\"Select a person\"\r\n        option-filter-prop=\"children\"\r\n        :filter-option=\"filterOption\"\r\n        @focus=\"handleFocus\"\r\n        @blur=\"handleBlur\"\r\n        @change=\"handleChange\"\r\n        style=\"width: 300px\"\r\n\r\n        \r\n    >\r\n        <a-select-option v-for=\"product in form.products\" :key=\"product\" :value=\"product\">\r\n            {{product.pname}}\r\n        </a-select-option>\r\n\r\n    </a-select>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动时间区间\">\r\n        <a-range-picker\r\n    :disabled-date=\"disabledDate\"\r\n    :disabled-time=\"disabledRangeTime\"\r\n    :show-time=\"{\r\n      hideDisabledOptions: true,\r\n      defaultValue: [moment('00:00:00', 'HH:mm:ss'), moment('11:59:59', 'HH:mm:ss')],\r\n    }\"\r\n    format=\"YYYY-MM-DD HH:mm:ss\"\r\n  />\r\n    </a-form-item>\r\n\r\n    \r\n    <a-form-item :wrapper-col=\"{ span: 14, offset: 4 }\">\r\n      <a-button type=\"primary\" @click=\"onSubmit\">\r\n        提交\r\n      </a-button>\r\n      <a-button style=\"margin-left: 10px;\" @click=\"BackList\">\r\n        返回\r\n      </a-button>\r\n    </a-form-item>\r\n  </a-form>\r\n</template>\r\n<script>\r\n\r\nimport moment from 'moment';\r\nexport default {\r\n  data() {\r\n    return {\r\n      labelCol: { span: 4 },\r\n      wrapperCol: { span: 14 },\r\n      form: {\r\n        name: '',\r\n        price: undefined,\r\n        num: undefined,\r\n        start_time: '',\r\n        end_time: '',\r\n        pid:undefined,\r\n        products:[],\r\n\r\n      },\r\n    };\r\n  },\r\n  mounted(){\r\n      this.getProduct()\r\n  },\r\n  methods: {\r\n\r\n\r\n\r\n        moment,\r\n    range(start, end) {\r\n        const result = [];\r\n        for (let i = start; i < end; i++) {\r\n            result.push(i);\r\n        }\r\n        return result;\r\n    },\r\n\r\n    disabledDate(current) {\r\n        // Can not select days before today and today\r\n        return current && current < moment().endOf('day');\r\n    },\r\n\r\n    disabledDateTime() {\r\n        return {\r\n            disabledHours: () => this.range(0, 24).splice(4, 20),\r\n            disabledMinutes: () => this.range(30, 60),\r\n            disabledSeconds: () => [55, 56],\r\n        };\r\n    },\r\n\r\n    disabledRangeTime(_, type) {\r\n      if (type === 'start') {\r\n        return {\r\n          disabledHours: () => this.range(0, 60).splice(4, 20),\r\n          disabledMinutes: () => this.range(30, 60),\r\n          disabledSeconds: () => [55, 56],\r\n        };\r\n      }\r\n      return {\r\n        disabledHours: () => this.range(0, 60).splice(20, 4),\r\n        disabledMinutes: () => this.range(0, 31),\r\n        disabledSeconds: () => [55, 56],\r\n      };\r\n    },\r\n\r\n\r\n\r\n      getProduct(){\r\n          this.$axios.get(\"/seckill/get_products\").then((rep) => {\r\n              console.log(rep);\r\n              this.form.products = rep.data.products\r\n\r\n          }).catch((rep) => {\r\n              console.log(rep);\r\n          })\r\n\r\n      },\r\n\r\n      BackList(){\r\n          this.$router.push(\"/main/seckill_list\")\r\n\r\n      },\r\n\r\n      \r\n    onSubmit() {\r\n        console.log(this.form.pid.id);\r\n\r\n        \r\n    },\r\n  },\r\n};\r\n</script>\r\n"]}]}