a-select>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动开始时间\">\r\n        <a-range-picker\r\n    :disabled-date=\"disabledDate\"\r\n    :disabled-time=\"disabledRangeTime\"\r\n    :show-time=\"{\r\n      hideDisabledOptions: true,\r\n      defaultValue: [moment('00:00:00', 'HH:mm:ss'), moment('11:59:59', 'HH:mm:ss')],\r\n    }\"\r\n    format=\"YYYY-MM-DD HH:mm:ss\"\r\n  />\r\n        <!-- <a-input v-model:value=\"form.start_time\" placeholder=\"请输入活动开始时间\"/> -->\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动结束时间\">\r\n      <a-input v-model:value=\"form.end_time\" placeholder=\"请输入活动结束时间\"/>\r\n    </a-form-item>\r\n\r\n    \r\n    <a-form-item :wrapper-col=\"{ span: 14, offset: 4 }\">\r\n      <a-button type=\"primary\" @click=\"onSubmit\">\r\n        提交\r\n      </a-button>\r\n      <a-button style=\"margin-left: 10px;\" @click=\"BackList\">\r\n        返回\r\n      </a-button>\r\n    </a-form-item>\r\n  </a-form>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      labelCol: { span: 4 },\r\n      wrapperCol: { span: 14 },\r\n      form: {\r\n        name: '',\r\n        price: undefined,\r\n        num: undefined,\r\n        start_time: '',\r\n        end_time: '',\r\n        pid:undefined,\r\n        products:[],\r\n\r\n      },\r\n    };\r\n  },\r\n  mounted(){\r\n      this.getProduct()\r\n  },\r\n  methods: {\r\n\r\n      getProduct(){\r\n          this.$axios.get(\"/seckill/get_products\").then((rep) => {\r\n              console.log(rep);\r\n              this.form.products = rep.data.products\r\n\r\n          }).catch((rep) => {\r\n              console.log(rep);\r\n          })\r\n\r\n      },\r\n\r\n      BackList(){\r\n          this.$router.push(\"/main/seckill_list\")\r\n\r\n      },\r\n    onSubmit() {\r\n\r\n        const form_data = new FormData();\r\n        form_data.append(\"name\",this.form.name)\r\n        form_data.append(\"price\",this.form.price)\r\n        form_data.append(\"num\",this.form.num)\r\n        form_data.append(\"unit\",this.form.unit)\r\n        form_data.append(\"pic\",document.querySelector('input[type=file]').files[0])\r\n        form_data.append(\"desc\",this.form.desc)\r\n\r\n        this.$axios.post(\"/seckill/product_add\",form_data).then((rep) => {\r\n            console.log(rep);\r\n            if (rep.data.code == 200){\r\n                alert(rep.data.msg)\r\n                this.$router.push(\"/main/product_list\")\r\n            }else {\r\n                alert(rep.data.msg)\r\n            }\r\n\r\n        }).catch((rep) => {\r\n            console.log(rep);\r\n\r\n        })\r\n\r\n      console.log('submit!', this.form);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}