        </div>\n        </router-link>\n      </li>\n\n    </ul>\n    <a href=\"\">\n    <div class=\"load_more\" @click=\"onLoadMore\">\n      <!-- <a-button  @click=\"onLoadMore\">\n          加载更多\n        </a-button> -->\n        加载更多\n    </div>\n    </a>\n        \n  </div>\n\n\n  \n</template>\n\n<script>\nexport default {\n  name: 'GoodsList',\n\n  data(){\n    return {\n      seckills:[]\n    }\n  },\n\n  mounted(){\n\n    this.getSeckillList()\n    window.addEventListener('scroll',this.scoll_get_data)\n  },\n\n\n  methods:{\n    getSeckillList(){\n      this.$axios.get(\"/seckill/front/get_seckill_list\").then((rep)=> {\n        console.log(rep);\n        this.seckills = rep.data.seckill_list\n\n\n      }).catch((rep) => {\n        console.log(rep);\n      })\n\n\n    },\n\n    scoll_get_data(){\n      // alert(11111)\n\n      // alert(document.documentElement.scrollHeightt)\n      // alert(document.body.offsetHeight)\n      // alert(window.innerHeigh)\n\n      // if ( document.documentElement.scrollHeight + window.innerHeight == document.body.offsetHeight){\n      //   alert(11111)\n      // }\n\n    },\n    \n    Logout(){\n      this.$auth.delFrontAuth()\n      this.$router.push(\"/login\")\n    }\n  }\n}\n</script>\n\n\n<style scoped lang=\"scss\">\n\n\n.load_more{\n  text-align: center;\n  margin-top: 30px;\n  background-color: rgb(198, 198, 224);\n}\n\n.page-title {\n  margin-left: -100px;\n  margin-bottom: 50px;\n  .left {\n    font-size: 32px;;\n  }\n  .right {\n    float:right;\n    font-size: 18px;;\n  }\n}\n.goods-list{\n  display: flex;\n  justify-content: flex-start;\n  flex-wrap: wrap;\n  .goods-box{\n    width: 25%;\n    height: 357px;\n    padding: 4px;\n    cursor: pointer;\n    box-sizing: border-box;\n    border: 1px solid #fff;\n    &:hover{\n      background-color: #f4f0ea;\n      border: 1px solid #dcdcdc;\n    }\n    .inner-box{\n      .thumbnail{\n        img{\n          width: 100%;\n          height: 240px;\n        }\n      }\n      .goods-info{\n        padding: 10px;\n        .price{\n          color: brown;\n          padding: 10px 0;\n          text-align: center;\n          font-size: 16px;\n          del {\n            text-decoration: line-through;\n            font-size: 14px;\n          }\n        }\n        .desc{\n          color: #666;\n        }\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}