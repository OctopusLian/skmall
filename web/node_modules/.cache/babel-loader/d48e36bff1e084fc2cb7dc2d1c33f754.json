S,CADyB;;AAAA,OAA3B,CA5Bc,EAiCd,YAAA,CAOc,sBAPd,EAOc;AAPA,uBAAa;AAAA,UAAA,IAAA,EAAA,EAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAOb,OAPd,EAAkD;0BAChD;AAAA,iBAEW,CAFX,YAAA,CAEW,mBAFX,EAEW;AAFD,YAAA,IAAI,EAAC,SAEJ;AAFe,YAAA,OAAK,EAAE,QAAA,CAAA;AAEtB,WAFX,E;8BAA2C;AAAA,qBAE3C,C,UAAA,CAF2C;AAAA,a;;WAA3C,E;;AAAA,Y,WAAA,CAEW,EACX,YAAA,CAEW,mBAFX,EAEW;AAFD,YAAA,KAA0B,EAA1B;AAAA,6BAAA;AAAA,aAEC;AAF2B,YAAA,OAAK,EAAE,QAAA,CAAA;AAElC,WAFX,E;8BAAuD;AAAA,qBAEvD,C,UAAA,CAFuD;AAAA,a;;WAAvD,E;;AAAA,Y,WAAA,CADW,CAFX;AAAA,S,CADgD;;AAAA,OAAlD,CAjCc,CAFd;AAAA,K;;GAFF,E;;AAAA,I,qCAAA,C","sourcesContent":["<template>\r\n  <a-form :model=\"form\" :label-col=\"labelCol\" :wrapper-col=\"wrapperCol\">\r\n\r\n    <a-form-item label=\"活动名称\">\r\n      <a-input v-model:value=\"form.name\" placeholder=\"请输入活动名称\" />\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动价格\">\r\n      <a-input v-model:value=\"form.price\" placeholder=\"请输入活动价格，保留两位小，不能大于商品价格\"/>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动库存\">\r\n      <a-input v-model:value=\"form.num\" placeholder=\"请输入活动库存\"/>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"选择关联商品\">\r\n      <a-select\r\n        v-model:value=\"form.pid\"\r\n        show-search\r\n        placeholder=\"Select a person\"\r\n        option-filter-prop=\"children\"\r\n        style=\"width: 300px\"\r\n    >\r\n        <a-select-option value=\"jack\">Jack</a-select-option>\r\n        <a-select-option value=\"lucy\">Lucy</a-select-option>\r\n        <a-select-option value=\"tom\">Tom</a-select-option>\r\n    </a-select>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动开始时间\">\r\n        <a-input v-model:value=\"form.start_time\" placeholder=\"请输入活动开始时间\"/>\r\n    </a-form-item>\r\n\r\n    <a-form-item label=\"活动结束时间\">\r\n      <a-input v-model:value=\"form.end_time\" placeholder=\"请输入活动结束时间\"/>\r\n    </a-form-item>\r\n\r\n    \r\n    <a-form-item :wrapper-col=\"{ span: 14, offset: 4 }\">\r\n      <a-button type=\"primary\" @click=\"onSubmit\">\r\n        提交\r\n      </a-button>\r\n      <a-button style=\"margin-left: 10px;\" @click=\"BackList\">\r\n        返回\r\n      </a-button>\r\n    </a-form-item>\r\n  </a-form>\r\n</template>\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      labelCol: { span: 4 },\r\n      wrapperCol: { span: 14 },\r\n      form: {\r\n        name: '',\r\n        price: undefined,\r\n        num: undefined,\r\n        start_time: '',\r\n        end_time: '',\r\n        pid:undefined,\r\n\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n      BackList(){\r\n          this.$router.push(\"/main/seckill_list\")\r\n\r\n      },\r\n    onSubmit() {\r\n\r\n        const form_data = new FormData();\r\n        form_data.append(\"name\",this.form.name)\r\n        form_data.append(\"price\",this.form.price)\r\n        form_data.append(\"num\",this.form.num)\r\n        form_data.append(\"unit\",this.form.unit)\r\n        form_data.append(\"pic\",document.querySelector('input[type=file]').files[0])\r\n        form_data.append(\"desc\",this.form.desc)\r\n\r\n        this.$axios.post(\"/product/product_add\",form_data).then((rep) => {\r\n            console.log(rep);\r\n            if (rep.data.code == 200){\r\n                alert(rep.data.msg)\r\n                this.$router.push(\"/main/product_list\")\r\n            }else {\r\n                alert(rep.data.msg)\r\n            }\r\n\r\n        }).catch((rep) => {\r\n            console.log(rep);\r\n\r\n        })\r\n\r\n      console.log('submit!', this.form);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}